<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.loy.mapper.LoyMapper">
    <resultMap id="loyMap" type="org.loy.entity.LoyEntity">
        <result property="name" column="name" ></result>
    </resultMap>

    <select id="listLoy" resultMap="loyMap">
        select name from t_loy
    </select>
</mapper>